    "app-prod": "NODE_ENV=production FE_ENV=prod webpack --config webpack.production.config.js --progress",
    "app-stg": "NODE_ENV=production FE_ENV=stg webpack --config webpack.staging.config.js --progress",
    "process-html": "node scripts/processHtml.js",
    "build-prod": "npm run dll && npm run app-prod && npm run process-html",
    "build-stg": "npm run dll && npm run app-stg && npm run process-html"
